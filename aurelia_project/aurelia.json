{
  "name": "tt-services-customer",
  "type": "project:application",
  "paths": {
    "root": "src",
    "resources": "resources",
    "elements": "resources/elements",
    "attributes": "resources/attributes",
    "valueConverters": "resources/value-converters",
    "bindingBehaviors": "resources/binding-behaviors"
  },
  "dependencies": [
    {
      "name": "fomantic-ui",
      "path": "../node_modules/fomantic-ui/dist",
      "main": "semantic.min.js",
      "resources": [
        "semantic.min.css"
      ]
    }
  ],
  "transpiler": {
    "id": "babel",
    "fileExtension": ".js"
  },
  "build": {
    "options": {
      "server": "dev",
      "extractCss": "prod",
      "coverage": false
    }
  },
  "platform": {
    "hmr": false,
    "open": false,
    "port": 8080,
    "host": "localhost",
    "output": "public"
  },
  "packageManager": "npm"
}
